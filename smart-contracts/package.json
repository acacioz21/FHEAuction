{
  "name": "fhevm-hardhat-template",
  "description": "Hardhat-based template for developing FHEVM Solidity smart contracts",
  "version": "0.1.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8.0.0"
  },
  "license": "BSD-3-Clause-Clear",
  "homepage": "https://github.com/zama-ai/fhevm-hardhat-template/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zama-ai/fhevm-hardhat-template.git"
  },
  "keywords": [
    "fhevm",
    "zama",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3",
    "typescript",
    "hardhat"
  ],
  "dependencies": {
    "encrypted-types": "^0.0.4"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "ethers": "^6.13.0",
    "hardhat": "^2.22.0",
    "hardhat-deploy": "^0.11.45",
    "mocha": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.0"
  },
  "files": [
    "contracts"
  ],
  "scripts": {
    "clean": "rimraf ./fhevmTemp ./artifacts ./cache ./coverage ./types ./coverage.json ./dist ./deployments",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "cross-env SOLIDITY_COVERAGE=true hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && pnpm run typechain",
    "lint": "pnpm run lint:sol && pnpm run lint:ts && pnpm run prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postcompile": "pnpm run typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "test": "hardhat test",
    "test:sepolia": "hardhat test --network sepolia",
    "build:ts": "tsc --project tsconfig.json",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "chain": "hardhat node --network hardhat --no-deploy",
    "deploy:localhost": "hardhat deploy --network localhost",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "deploy:voting": "hardhat deploy --network sepolia --tags FHEVoting",
    "deploy:counter": "hardhat deploy --network sepolia --tags FHECounter",
    "deploy:bank": "hardhat deploy --network sepolia --tags FHEBank",
    "verify:sepolia": "hardhat verify --network sepolia",
    "verify:sourcify": "hardhat sourcify --network sepolia",
    "verify:all": "pnpm run verify:sepolia && pnpm run verify:sourcify",
    "verify:counter": "hardhat verify --network sepolia 0xaD920A4E9ACD84aA5F094e128b0d811eDB12F57F",
    "verify:voting": "hardhat verify --network sepolia 0x8eAf5350f6E26051f7902109BD3a8709abB6Fb14",
    "verify:bank": "hardhat verify --network sepolia 0xA020287B1670453919C2f49e2e8c2C09B96101B8",
    "verify:sourcify:counter": "hardhat sourcify --network sepolia 0xaD920A4E9ACD84aA5F094e128b0d811eDB12F57F",
    "verify:sourcify:voting": "hardhat sourcify --network sepolia 0x8eAf5350f6E26051f7902109BD3a8709abB6Fb14",
    "verify:sourcify:bank": "hardhat sourcify --network sepolia 0xA020287B1670453919C2f49e2e8c2C09B96101B8",
    "verify:all:contracts": "pnpm run verify:counter && pnpm run verify:voting && pnpm run verify:bank",
    "verify:all:sourcify": "pnpm run verify:sourcify:counter && pnpm run verify:sourcify:voting && pnpm run verify:sourcify:bank"
  },
  "overrides": {
    "ws@>=7.0.0 <7.5.10": ">=7.5.10",
    "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
    "elliptic@>=4.0.0 <=6.5.6": ">=6.5.7",
    "elliptic@>=2.0.0 <=6.5.6": ">=6.5.7",
    "elliptic@>=5.2.1 <=6.5.6": ">=6.5.7",
    "micromatch@<4.0.8": ">=4.0.8",
    "elliptic@<6.6.0": ">=6.6.0",
    "elliptic@<6.5.6": ">=6.5.6",
    "undici@>=6.0.0 <6.21.1": ">=6.21.1",
    "undici@>=4.5.0 <5.28.5": ">=5.28.5",
    "elliptic@<=6.6.0": ">=6.6.1",
    "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2",
    "axios@>=1.0.0 <1.8.2": ">=1.8.2",
    "axios@<0.29.1": ">=0.29.1",
    "cookie@<0.7.0": ">=0.7.0",
    "minimatch": "^3.1.2"
  }
}
